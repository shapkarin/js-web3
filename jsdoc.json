{
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.(js|jsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "node_modules/jsdoc-babel",
    "plugins/markdown"
  ],
  "babel": {
    "presets": ["@babel/preset-react"]
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./public/jsdoc/",
    "readme": "./README.md",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true
  }
}
